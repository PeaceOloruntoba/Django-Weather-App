{
  "version": 2,
  "builds": [
    {
      "src": "weather/wsgi.py"
      "use": "@vercel/python",
      "config": { "maxlambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh"
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "weather/wsgi.py"
    }
  ]
}
